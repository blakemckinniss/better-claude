{
  "mcp_mappings": {
    "mcp__github__": {
      "name": "GitHub MCP",
      "priority": 8,
      "keywords": {
        "10": [
          "my-org github",
          "company repo"
        ],
        "8": [
          "deploy to prod",
          "release branch"
        ]
      },
      "negative_patterns": [
        "test repo",
        "example github"
      ]
    },
    "mcp__custom_tool__": {
      "name": "Custom Tool",
      "priority": 7,
      "keywords": {
        "9": [
          "custom keyword",
          "special term"
        ],
        "5": [
          "general term"
        ]
      }
    }
  },
  "domain_keywords": {
    "my_workflow": {
      "keywords": [
        "specific workflow",
        "company process"
      ],
      "suggested_tools": [
        "mcp__github__",
        "mcp__custom_tool__"
      ]
    }
  },
  "tool_combinations": {
    "custom_flow": {
      "patterns": [
        "pattern.*to.*match"
      ],
      "tools": [
        "mcp__tool1__",
        "mcp__tool2__"
      ],
      "boost": 15
    }
  }
}